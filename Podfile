target 'UTShuttleSwift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'SwiftSocket'
  pod 'SwiftyUserDefaults'
  pod 'Alertift'

  # Pods for UTShuttleSwift

  target 'UTShuttleSwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'UTShuttleSwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
